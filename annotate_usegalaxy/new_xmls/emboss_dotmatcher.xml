<tool id="EMBOSS: dotmatcher24" name="dotmatcher" version="5.0.0.1">
  <!--This tool descriptor has been annotated by ToolDog v0.3.1-->
  <description>Displays a thresholded dotplot of two sequences</description>
  <edam_operations>
    <edam_operation>operation_2403</edam_operation>
    <edam_operation>operation_0495</edam_operation>
    <edam_operation>operation_0258</edam_operation>
    <edam_operation>operation_0496</edam_operation>
    <edam_operation>operation_0292</edam_operation>
  </edam_operations>
  <edam_topics>
    <edam_topic>topic_3047</edam_topic>
    <edam_topic>topic_0080</edam_topic>
    <edam_topic>topic_3070</edam_topic>
  </edam_topics>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <version_command/>
  <command><![CDATA[perl '$__tool_directory__/emboss_single_outputfile_wrapper.pl' dotmatcher -asequence '$input1' -bsequence '$input2' -goutfile '$out_file1' -windowsize $windowsize -threshold $threshold -graph png -xygraph png -auto]]></command>
