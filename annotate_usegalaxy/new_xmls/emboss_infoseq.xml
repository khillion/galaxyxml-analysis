<tool id="EMBOSS: infoseq46" name="infoseq" version="5.0.0">
  <!--This tool descriptor has been annotated by ToolDog v0.3.1-->
  <description>Displays some simple information about sequences</description>
  <edam_operations>
    <edam_operation>operation_2403</edam_operation>
    <edam_operation>operation_0495</edam_operation>
    <edam_operation>operation_0258</edam_operation>
    <edam_operation>operation_0496</edam_operation>
    <edam_operation>operation_0292</edam_operation>
  </edam_operations>
  <edam_topics>
    <edam_topic>topic_3047</edam_topic>
    <edam_topic>topic_0080</edam_topic>
    <edam_topic>topic_3070</edam_topic>
  </edam_topics>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <version_command/>
  <command><![CDATA[infoseq -sequence '$input1' -outfile '$out_file1' -html $html_out1 -heading $heading -usa $usa -name $disname -accession $accession -gi $gi -version $version -type $type -length $length -pgc
  $pgc -description $description -auto]]></command>
  <inputs>
    <param argument="input1" format="data" label="Sequences" name="input1" type="data"/>
    <param argument="html_out1" label="Format output as an HTML table" name="html_out1" type="select">
      <option value="no">No</option>
      <option value="yes">Yes</option>
    </param>
    <param argument="heading" label="Display column headings" name="heading" type="select">
      <option value="yes">Yes</option>
      <option value="no">No</option>
    </param>
    <param argument="usa" label="Display the USA of the sequence" name="usa" type="select">
      <option value="yes">Yes</option>
      <option value="no">No</option>
    </param>
    <param argument="disname" label="Display 'name' column" name="disname" type="select">
      <option value="yes">Yes</option>
      <option value="no">No</option>
    </param>
    <param argument="accession" label="Display 'accession' column" name="accession" type="select">
      <option value="yes">Yes</option>
      <option value="no">No</option>
    </param>
    <param argument="gi" label="Display 'GI' column" name="gi" type="select">
      <option value="no">No</option>
      <option value="yes">Yes</option>
    </param>
    <param argument="version" label="Display 'version' column" name="version" type="select">
      <option value="no">No</option>
      <option value="yes">Yes</option>
    </param>
    <param argument="type" label="Display 'type' column" name="type" type="select">
      <option value="yes">Yes</option>
      <option value="no">No</option>
    </param>
    <param argument="length" label="Display 'length' column" name="length" type="select">
      <option value="yes">Yes</option>
      <option value="no">No</option>
    </param>
    <param argument="pgc" label="Display 'percent GC content' column" name="pgc" type="select">
      <option value="yes">Yes</option>
      <option value="no">No</option>
    </param>
    <param argument="description" label="Display 'description' column" name="description" type="select">
      <option value="yes">Yes</option>
      <option value="no">No</option>
    </param>
  </inputs>
  <outputs>
    <data format="txt" hidden="false" name="out_file1"/>
  </outputs>
  <help><![CDATA[
    You can view the original documentation here_.

    .. _here: http://galaxy-iuc.github.io/emboss-5.0-docs/infoseq.html
  ]]></help>
  <citations/>
</tool>
